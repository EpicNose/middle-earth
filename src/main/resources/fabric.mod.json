{
  "schemaVersion": 1,
  "id": "me",
  "version": "${version}",

  "name": "Middle-earth",
  "description": "Join the legendary world of Tolkien's universe from The Lord of The Rings to the Hobbit and more!",
  "authors": [
    " Lead Dev: Jukoz, ObliviousCrab\n\nDeveloppers: Jukoz, ObliviousCrab, Slooshyboi, TomSchlom, Froosty11, nullBlade\n\nArtists: Boenndal, Jooble, Sindavar, Thijs, Jukoz, Khuz, ObliviousCrab, R3tt0, Thorin_The_III, Tom, WorseNotePad, JB3, Tyn, Python_200, Ag3ntCrab\n\nBuilders: Boenndal, Thijs, Slooshyboi, Jooble, Jukoz, ObliviousCrab, JB3, Grandison"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/middle-earth",
    "sources": "https://github.com/Jukoz/middle-earth",
    "issues": "https://github.com/Jukoz/middle-earth/issues"
  },

  "license": "MIT",
  "icon": "assets/me/icon.png",

  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.jukoz.me.datageneration.DataGeneration"
    ],
    "main": [
      "net.jukoz.me.MiddleEarth"
    ],
    "client": [
      "net.jukoz.me.MiddleEarthClient"
    ],
    "rei_client": [
      "net.jukoz.me.compat.ModREIClientPlugin"
    ]
  },
  "mixins": [
    "me.mixins.json"
  ],

  "accessWidener" : "me.accesswidener",

  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-api": "*",
    "minecraft": "~1.21.1",
    "java": ">=21"
  },
  "suggests": {
    "roughlyenoughitems": "*",
    "modmenu": "*"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.wiki": "https://middleearthmcmod.wiki.gg/wiki/Middle_Earth_Minecraft_Mod_Wiki",
        "modmenu.discord": "https://discord.gg/9yQ7UWkVUz",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/middle-earth",
        "modmenu.modrinth": "https://modrinth.com/mod/middle-earth"
      },
      "update_checker": true
    }
  }
}
