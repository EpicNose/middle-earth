{
  "schemaVersion": 1,
  "id": "me",
  "version": "${version}",

  "name": "Middle-earth",
  "description": "Bringing Middle-earth to Minecraft!",
  "authors": [
    "Jukoz, Krahab"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/Jukoz/me"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.jukoz.me.datageneration.DataGeneration"
    ],
    "main": [
      "net.jukoz.me.MiddleEarth"
    ],
    "client": [
      "net.jukoz.me.MiddleEarthClient"
    ],
    "rei_client": [
      "net.jukoz.me.compat.ModREIClientPlugin"
    ]
  },
  "mixins": [
    "me.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
